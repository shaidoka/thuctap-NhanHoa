    "import numpy as np\n",
    "from numpy.random import randn\n",
    "\n",
    "mean = 0\n",
    "stdev = 1\n",
    "N = 10000\n",
    "count = 0\n",
    "for i in randn(N):\n",
    "    if i > -1 and i < 1:\n",
    "        count = count + stdev\n",
    "mean = count / N * 100\n",
    "print(str(mean) + \"%\")"
   ]
